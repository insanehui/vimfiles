snippet utf "filet link" 
<url:./filets/$0>
endsnippet

snippet uts "使用unite everything来定位" 
<url:vimscript:exe \"normal \\\\%3cBslash%3ex${1:filepath}\\\\%3ccr%3egg/${2:search_text}\\\\%3ccr%3ezz\">
endsnippet

snippet uth "utl vimhelp" 
<url:vimhelp:${1:help}>
endsnippet

snippet uti "utl set id"
<?utl:id=$0?>
endsnippet

snippet utr "utl id"
<url:#r=$0>
endsnippet

snippet utl "utl's url"
<url:${0:${VISUAL}}>
endsnippet

snippet utv "utl vimscript" 
<url:vimscript:${0:ls}>
endsnippet

