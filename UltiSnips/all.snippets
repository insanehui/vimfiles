extends ansicolor

#snippet { "{ } completion" w
#{
#	${0:${VISUAL}}
#}
#endsnippet

snippet { "{}" w
{${1:${VISUAL}}}$0
endsnippet

#snippet [ "[ ]" w
#[
#	${0:${VISUAL}}
#]
#endsnippet

snippet [ "[]" w
[${1:${VISUAL}}]$0
endsnippet

snippet utl "utl's url"
<url:${0:${VISUAL}}>
endsnippet

snippet uti "utl set id"
<?utl:id=$0?>
endsnippet

snippet utr "utl id"
<url:#r=$0>
endsnippet

